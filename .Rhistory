##change government spreads from factors
View(EuropeOmnibus)
EuropeDefSpread <- subset(EuropeOmnibus, !is.na(DefSpendDelt_lead))
EuropeDefSpread <- subset(EuropeDefSpread, !is.na(DefSpread))
View(EuropeDefSpread)
EuropeDefSpread <- subset(EuropeDefSpread, !Country %in% c("Serbia", "Bulgaria", "Romania", "Serbia", "Slovakia"))
EuropeDefSpread_lag1 <- subset(EuropeDefSpread, !is.na(DefSpread_lag1))
pdata <- plm.data(EuropeDefSpread_lag1, index=c("Country", "Year"))
EUDefSpreadResults8 <- lm(DefSpendDelt_lead ~ DefSpread_lag1 + GDPpCapDelt, EuropeDefSpread_lag1)
summary(EUDefSpreadResults8)
screenreg(list(EUDefSpreadResults1,EUDefSpreadResults3, EUDefSpreadResults8),
custom.model.names=c("PubOp_0","PubOp_1", "GDP"))
EUDefSpreadResults8 <- lm(DefSpendDelt_lead ~ DefSpread_lag1 + GDPpCapDelt, EuropeDefSpread_lag1)
DefSpreadPooled <- plm(DefSpendDelt_lead ~ DefSpread_lag1 + GDPpCapDelt, data = pdata, model = "pooling")
summary(DefSpreadPooled)
plmtest(DefSpreadPooled) #if pvalue small, go ahead and estimate a random effects model. "alternative hypothesis, significant effects"
DefSpreadRandom <- plm(DefSpendDelt_lead ~ DefSpread_lag1 + GDPpCapDelt, data = pdata, model = "random")
DefSpreadBetween <- plm(DefSpendDelt_lead ~ DefSpread_lag1 + GDPpCapDelt, data = pdata, model = "between")
summary(DefSpreadBetween)
screenreg(list(EUDefSpreadResults8, DefSpreadPooled, DefSpreadBetween),
custom.model.names = c("OLS", "Pooled", "Between"))
DefSpreadFD <- plm(DefSpendDelt_lead ~ DefSpread_lag1 + GDPpCapDelt, data = pdata, model = "fd") ##when variables don't vary from one year to the next, the variable is dropped because the first difference takes difference of the model averaged over time from the original model and for these variables the value would be 0.
screenreg(list(EUDefSpreadResults8, DefSpreadPooled, DefSpreadBetween, DefSpreadFD),
custom.model.names = c("OLS", "Pooled", "Between", "FD"))
DefSpreadWithin <- plm(DefSpendDelt_lead ~ DefSpread_lag1 + GDPpCapDelt, data =  pdata, model = "within")
summary(DefSpreadWithin)
screenreg(list(EUDefSpreadResults8, DefSpreadPooled, DefSpreadBetween, DefSpreadFD, DefSpreadWithin),
custom.model.names = c("OLS", "Pooled", "Between", "FD", "within"))
DefSpreadRandom <- plm(DefSpendDelt_lead ~ DefSpread_lag1 + GDPpCapDelt, data = pdata, model = "random")
screenreg(list(EUDefSpreadResults8, DefSpreadPooled, DefSpreadBetween, DefSpreadFD, DefSpreadWithin, DefSpreadRandom),
custom.model.names = c("OLS", "Pooled", "Between", "FD", "within", "Random"))
plmtest(DefSpreadPooled) #if pvalue small, go ahead and estimate a random effects model. "alternative hypothesis, significant effects"
##fixed effects versus OLS model
pFtest(DefSpreadWithin, DefSpreadPooled)
##p = .168 cannot reject the null that the pooling model works (all coefficients for each country should be the same ever year) if p value low, supports the within model(accept alternative hypothesis: significant effects)
phtest(DefSpreadRandom, DefSpreadWithin)
View(pdata)
options(error=recover)
options(warn=1)
